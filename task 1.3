# ax^2 + bx + c = 0

a = float(input("Введіть a: "))
b = float(input("Введіть b: "))
c = float(input("Введіть c: "))

D = b * b - 4 * a * c

if D > 0:
    x1 = (-b + D ** 0.5) / (2 * a)
    x2 = (-b - D ** 0.5) / (2 * a)
    print("Два корені:")
    print("x1 =", x1)
    print("x2 =", x2)

elif D == 0:
    x = -b / (2 * a)
    print("Один корінь:")
    print("x =", x)

else:
    print("Коренів немає")
