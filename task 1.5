n = int(input("Введіть кількість елементів масиву: "))

arr = []

for i in range(n):
    value = int(input("Введіть елемент: "))
    arr.append(value)

for i in range(n):
    for j in range(0, n - i - 1):
        if arr[j] > arr[j + 1]:
            temp = arr[j]
            arr[j] = arr[j + 1]
            arr[j + 1] = temp

print("Відсортований масив:")
print(arr)
