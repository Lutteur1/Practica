import math

def rectangle_area(a, b):
    return a * b

def circle_area(r):
    return math.pi * r * r

a = float(input("Введіть сторону прямокутника a: "))
b = float(input("Введіть сторону прямокутника b: "))
r = float(input("Введіть радіус кола: "))

print("Площа прямокутника =", rectangle_area(a, b))
print("Площа кола =", circle_area(r))
